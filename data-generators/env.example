# Data Generation Environment Variables
# Copy this file to .env and modify as needed
# Override config.yaml settings with environment variables

# Basic Settings
DATA_DOMAIN=tech
DATA_VOLUME=medium
DATA_COMPLEXITY=intermediate

# Output Settings
OUTPUT_DIR=../backend/data
OVERWRITE_EXISTING=false

# Content Settings
DATA_LANGUAGE=en
INCLUDE_EXAMPLES=true
INCLUDE_TROUBLESHOOTING=true

# Tech Domain Specific
TECH_FOCUS_AREAS=api,database,authentication,deployment,monitoring
TECH_STACK=python,fastapi,postgresql,docker,kubernetes
TECH_COMMON_ISSUES=timeout,authentication,database_connection,deployment_failure

# Healthcare Domain Specific
HEALTHCARE_FOCUS_AREAS=patient_records,hipaa,medical_protocols,compliance,telemedicine
HEALTHCARE_SPECIALTIES=cardiology,oncology,emergency,pediatrics,radiology
HEALTHCARE_COMPLIANCE_STANDARDS=hipaa,fda,joint_commission,meaningful_use

# Finance Domain Specific
FINANCE_FOCUS_AREAS=payments,risk_management,compliance,fraud_detection,trading
FINANCE_PRODUCTS=banking,investment,insurance,fintech,cryptocurrency
FINANCE_REGULATIONS=pci_dss,sox,basel_iii,gdpr,aml

# Ecommerce Domain Specific
ECOMMERCE_FOCUS_AREAS=inventory,orders,payments,customer_service,analytics
ECOMMERCE_PLATFORMS=shopify,woocommerce,custom,marketplace,mobile
ECOMMERCE_BUSINESS_MODELS=b2c,b2b,marketplace,subscription,dropshipping

# Generation Settings
USE_FAKER=true
REALISTIC_DATA=true
INCLUDE_METADATA=true
RANDOMIZE_CONTENT=true
SEED=null
